# Queue: Using Array as collection:
# Implement a class Queue=()


class Queue:
    def __init__(self):
        self.items = []

    def is_empty(self):
        if len(self.items) == 0:
            return self.items == []
        else:
            print("everything is ok")

    def enqueue(self, item):
        self.items.insert(0, item)

    def dequeue(self):
        return self.items.pop()

    def size(self):
        return len(self.items)


def main():
    q = Queue()
    q.enqueue(4)

    q.enqueue('bam')
    q.enqueue(True)

    print(q.size())
    print(Queue)


main()

