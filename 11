# Queue: Using Array as collection:
# Implement a class Queue=()

class Queue:
    def __init__(self):
        self.items = []

    def isEmpty(self):
        return self.items == []

    def enqueue(self, item):
        self.items.insert(0,item)

    def dequeue(self):
        return self.items.pop()

    def size(self):
        return len(self.items)

def main():
    q = Queue()
    q.enqueue(4)

    q.enqueue('bam')
    q.enqueue(True)

    print(q.size())
    print(Queue)

main()
